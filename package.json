{
  "name": "urbanrecreation",
  "version": "1.0.0",
  "description": "Recreate Urban Rivals",
  "main": "build/main.js",
  "type": "module",
  "scripts": {
    "run-js": "cd src/js && node --inspect=9222 main.mjs",
    "run-ts": "cd build/ts && node --inspect-brk=9229 --max-old-space-size=22000 --experimental-specifier-resolution=node --enable-source-maps Main.js",
    "server": "tsc && cd build/ts && node --max-old-space-size=22000 --experimental-specifier-resolution=node --enable-source-maps solver/Server.js",
    "dev": "tsc && npm run run-ts",
    "test": "jest --coverage",
    "lint": "./node_modules/.bin/eslint src/ts",
    "sim-limit": "cd src/js && node --inspect-brk=9222 --max-old-space-size=16384 main.mjs",
    "api": "node src/api/UR_API.js"
  },
  "keywords": [
    "urbanrivals",
    "nodejs"
  ],
  "author": "ArmaanAS",
  "license": "ISC",
  "dependencies": {
    "colors": "^1.4.0",
    "dotenv": "^10.0.0",
    "oauth": "^0.9.15",
    "urban-rivals-oauth": "^1.2.1",
    "ws": "^7.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@types/jest": "^27.0.2",
    "@types/ws": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^8.0.1",
    "jest": "^27.3.1",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  }
}